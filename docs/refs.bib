	file = {arXiv\:1902.04996 PDF:/Users/magnusmunch/Zotero/storage/67UUQI9N/Zhao and Zucknick - 2019 - Structured penalized regression for drug sensitivi.pdf:application/pdf;arXiv.org Snapshot:/Users/magnusmunch/Zotero/storage/EBFGJH2C/1902.html:text/html}
}

