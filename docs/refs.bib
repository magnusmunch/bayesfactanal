	keywords = {Statistics - Machine Learning},
	file = {arXiv\:1506.03431 PDF:/Users/magnusmunch/Zotero/storage/SD28TT5X/Kucukelbir et al. - 2015 - Automatic Variational Inference in Stan.pdf:application/pdf;arXiv\:1506.03431 PDF:/Users/magnusmunch/Zotero/storage/AGRZSAMM/Kucukelbir et al. - 2015 - Automatic Variational Inference in Stan.pdf:application/pdf;arXiv.org Snapshot:/Users/magnusmunch/Zotero/storage/UUE6QG6U/1506.html:text/html;arXiv.org Snapshot:/Users/magnusmunch/Zotero/storage/EAE25QPC/1506.html:text/html}
