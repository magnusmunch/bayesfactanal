	volume = {59},
	copyright = {© 2017 WILEY‐VCH Verlag GmbH \& Co. KGaA, Weinheim},
	issn = {1521-4036},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/bimj.201600090},
	doi = {10.1002/bimj.201600090},
	abstract = {Reconstruction of a high-dimensional network may benefit substantially from the inclusion of prior knowledge on the network topology. In the case of gene interaction networks such knowledge may come for instance from pathway repositories like KEGG, or be inferred from data of a pilot study. The Bayesian framework provides a natural means of including such prior knowledge. Based on a Bayesian Simultaneous Equation Model, we develop an appealing Empirical Bayes (EB) procedure that automatically assesses the agreement of the used prior knowledge with the data at hand. We use variational Bayes method for posterior densities approximation and compare its accuracy with that of Gibbs sampling strategy. Our method is computationally fast, and can outperform known competitors. In a simulation study, we show that accurate prior data can greatly improve the reconstruction of the network, but need not harm the reconstruction if wrong. We demonstrate the benefits of the method in an analysis of gene expression data from GEO. In particular, the edges of the recovered network have superior reproducibility (compared to that of competitors) over resampled versions of the data.},
	language = {en},
	number = {5},
	journal = {Biometrical Journal},
	author = {Kpogbezan, Gino B. and Vaart, Aad W. van der and Wieringen, Wessel N. van and Leday, Gwenaël G. R. and Wiel, Mark A. van de},
	month = sep,
	year = {2017},
	keywords = {Empirical Bayes, Statistics - Methodology, Variational approximation, High-dimensional Bayesian inference, Prior information, Undirected network},
	pages = {932--947},
	file = {arXiv\:1605.07514 PDF:/Users/magnusmunch/Zotero/storage/T6G8XUUJ/Kpogbezan et al. - 2016 - An empirical Bayes approach to network recovery us.pdf:application/pdf;arXiv.org Snapshot:/Users/magnusmunch/Zotero/storage/CN9NRWDP/1605.html:text/html;bimj201600090-sup-0001-SuppMaterial.pdf:/Users/magnusmunch/Zotero/storage/GKWNDLTE/bimj201600090-sup-0001-SuppMaterial.pdf:application/pdf;Full Text PDF:/Users/magnusmunch/Zotero/storage/TXZ6JNSR/Kpogbezan et al. - 2017 - An empirical Bayes approach to network recovery us.pdf:application/pdf;Snapshot:/Users/magnusmunch/Zotero/storage/GAXAWSSG/bimj.html:text/html}
